/// @description option_refresh_keys()
function option_refresh_keys() {

	key[0] = global.leftKey;
	key[1] = global.rightKey;
	key[2] = global.upKey;
	key[3] = global.downKey;
	key[4] = global.shootKey;
	key[5] = global.jumpKey;
	key[6] = global.dashKey;
	key[7] = global.specialKey;
	key[8] = global.weaponSwitchRightKey;
	key[9] = global.weaponSwitchLeftKey;
	key[10] = global.gigaKey;



}
